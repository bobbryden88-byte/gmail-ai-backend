{
  "version": 2,
  "buildCommand": "npm install && npx prisma generate",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "functions": {
    "src/app.js": {
      "maxDuration": 30
    }
  }
}
